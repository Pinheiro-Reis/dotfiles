// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+space",
        "command": "workbench.action.openView"
    },
    {
        "key": "ctrl+enter",
        "command": "-github.copilot.generate",
        "when": "editorTextFocus && github.copilot.activated && !commentEditorFocused"
    }
]